<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>main</title>
</head>
<body>
  <h1>main</h1>
  <p>hello, <strong><%= username %></strong></p>
  <% if(usernames) { %>
    <ul id="userlist">
      <% usernames.forEach(function(username) { %>
      <li>
        <a href="/users/<%= username %>"><%= username %></a>
      </li>
      <% }); %>
    </ul>
  <% } %>
  <a href="/logout">LOGOUT</a>
</body>
</html>
